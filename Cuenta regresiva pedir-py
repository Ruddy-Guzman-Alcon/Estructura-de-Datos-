def cuenta_regresiva(numero):
     numero -= 1
     if numero > 0:
         print (numero)
         cuenta_regresiva(numero)
        
     else: 
         print ("Esta Mal choco!")
     print ("Fin de la funci√≥n", numero)

x=int(input("Digite un numero please : "))
cuenta_regresiva(x)
    
    
  
